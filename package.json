{
  "name": "funcdraw-monorepo",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "fd-cli",
    "fd-player",
    "funcdraw"
  ],
  "scripts": {
    "build": "npm run build --workspaces --if-present",
